{
    "name": "audentio/laravel-base",
    "description": "Laravel base for projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jake Booher",
            "email": "jakeb@audentio.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "illuminate/contracts": "^7.0 | ^8.0",
        "illuminate/support": "^7.0 | ^8.0",
        "laravel/framework": "^7.0 | ^8.0",
        "ramsey/uuid": "^4"
    },
    "autoload": {
        "psr-4": {
            "Audentio\\LaravelBase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Audentio\\LaravelBase\\Providers\\ExtendServiceProvider",
                "Audentio\\LaravelBase\\Providers\\LaravelBaseServiceProvider",
                "Audentio\\LaravelBase\\Providers\\ValidationServiceProvider"
            ]
        }
    }
}
